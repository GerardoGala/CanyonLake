<?xml version="1.0" encoding="utf-8"?>
<ManagedExtension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="FacebookLike">
  <AdminPage />
  <Author>&lt;a href="http://www.isharpnote.com/"&gt;ISHARPNOTE&lt;/a&gt;</Author>
  <Description>Helps in like your Post with Facebook</Description>
  <Enabled>true</Enabled>
  <Priority>0</Priority>
  <Settings>
    <Delimiter>44</Delimiter>
    <Help>Enable facebook Like button on Post Home page and Post page</Help>
    <Hidden>false</Hidden>
    <Index>0</Index>
    <IsScalar>true</IsScalar>
    <KeyField>ShowPostList</KeyField>
    <Name>FacebookLike</Name>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Display Like button on featured list</Label>
      <MaxLength>20</MaxLength>
      <Name>ShowPostList</Name>
      <ParamType>Boolean</ParamType>
      <Required>false</Required>
      <SelectedValue />
      <Values>True</Values>
    </Parameters>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Show Faces (Show profile pictures below the button)</Label>
      <MaxLength>20</MaxLength>
      <Name>ShowFaces</Name>
      <ParamType>Boolean</ParamType>
      <Required>false</Required>
      <SelectedValue />
      <Values>False</Values>
    </Parameters>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Width (the width of the plugin in pixel)</Label>
      <MaxLength>20</MaxLength>
      <Name>Width</Name>
      <ParamType>String</ParamType>
      <Required>false</Required>
      <SelectedValue />
      <Values />
    </Parameters>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Verb to display</Label>
      <MaxLength>20</MaxLength>
      <Name>Action</Name>
      <ParamType>DropDown</ParamType>
      <Required>false</Required>
      <SelectedValue>like</SelectedValue>
      <Values>like</Values>
      <Values>recommend</Values>
    </Parameters>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Color Scheme (color scheme of the plugin)</Label>
      <MaxLength>20</MaxLength>
      <Name>ColorScheme</Name>
      <ParamType>DropDown</ParamType>
      <Required>false</Required>
      <SelectedValue>light</SelectedValue>
      <Values>light</Values>
      <Values>dark</Values>
    </Parameters>
    <Parameters>
      <KeyField>false</KeyField>
      <Label>Layout Style</Label>
      <MaxLength>20</MaxLength>
      <Name>SelectedLayout</Name>
      <ParamType>DropDown</ParamType>
      <Required>false</Required>
      <SelectedValue>standard</SelectedValue>
      <Values>standard</Values>
      <Values>button_count</Values>
      <Values>box_count</Values>
    </Parameters>
    <ShowAdd>true</ShowAdd>
    <ShowDelete>true</ShowDelete>
    <ShowEdit>true</ShowEdit>
  </Settings>
  <ShowSettings>true</ShowSettings>
  <Version>1.0</Version>
</ManagedExtension>